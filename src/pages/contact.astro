---
import ContactForm from "@/components/forms/Contact.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "Contact Hermosa Software: Get in Touch for Innovative Web Solutions",
	description:
		"Reach out to Hermosa Software for expert web development services. Connect with us to discuss your project and explore cutting-edge web solutions.",
	canonical: "https://hermosasoftware.io/contact/",
};
---

<BaseLayout seo={seo}>
	<ContactForm />
</BaseLayout>
